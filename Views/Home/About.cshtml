@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>
<script src="https://cdn.jsdelivr.net/npm/vue@2/dist/vue.js"></script>
<p>Use this area to provide additional information.</p>

<div id="app">
    {{ message }}
    <input type="text" v-model="message" />
</div>


<script>
    var app = new Vue({
        el: '#app',
        data: {
            message: 'Hello Vue!'
        }
    })
</script>
